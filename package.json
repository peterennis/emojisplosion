{
  "name": "emojisplosion",
  "version": "1.2.0",
  "description": "Blasts emoji like fireworks all up in your HTML page.",
  "main": "src/index.js",
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint --config tslint.json --format stylish --project tsconfig.json",
    "verify": "npm run tsc && npm run tslint & npm run webpack",
    "watch": "chokidar \"src/**/*.ts\" -i \"src/**/*.d.ts\" -c \"npm run webpack\"",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuakgoldberg/emojisplosion.git"
  },
  "keywords": [
    "emoji",
    "explosion",
    "fireworks",
    "page",
    "all",
    "up",
    "in",
    "there",
    "dom"
  ],
  "author": "Josh Goldberg <joshuakgoldberg@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshuakgoldberg/emojisplosion/issues"
  },
  "homepage": "https://github.com/joshuakgoldberg/emojisplosion#readme",
  "devDependencies": {
    "chokidar": "^3.1.1",
    "chokidar-cli": "^2.0.0",
    "requirejs": "2.3.6",
    "run-p": "0.0.0",
    "ts-loader": "^6.2.0",
    "tslint": "5.20.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
